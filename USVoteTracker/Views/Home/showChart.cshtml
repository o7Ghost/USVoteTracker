@model IEnumerable<USVoteTracker.Models.Candidate>

@{
    List<String> xVal = new List<string>();
    List<int> yVal = new List<int>();

    foreach (var item in Model)
    {
        xVal.Add(item.Name);
        yVal.Add(item.Votes);
    };

    var myChart = new Chart(width: 600, height: 400)
        .AddTitle("Number of Votes")
        .AddSeries(
            name: "Candidates",
            xValue: xVal,
            yValues: yVal)
        .Write();
}